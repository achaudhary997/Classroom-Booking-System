                <items>
                    <FXCollections fx:factory="observableArrayList">
                        <String fx:value="CSE" />
                        <String fx:value="ECE" />
                        <String fx:value="CSAM" />
                    </FXCollections>
                </items>
            </ChoiceBox>
         </children>
      </VBox>
      <VBox layoutX="251.0" layoutY="96.0" prefHeight="412.0" prefWidth="156.0" spacing="10">
