            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="31.0" prefWidth="156.0" text="First Name" />
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="31.0" prefWidth="156.0" text="Last Name" textAlignment="CENTER" />
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="31.0" prefWidth="156.0" text="Email ID" textAlignment="CENTER" />
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="31.0" prefWidth="156.0" text="Date of Birth" textAlignment="CENTER" />
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="31.0" prefWidth="156.0" text="Phone Number" textAlignment="CENTER" />
            <Label alignment="CENTER" prefHeight="30.0" prefWidth="156.0" text="Password" />
            <Label alignment="CENTER" prefHeight="32.0" prefWidth="156.0" text="Enter Password Again" />
            <Label alignment="CENTER" prefHeight="32.0" prefWidth="156.0" text="Account Type" />
            <Label alignment="CENTER" prefHeight="30.0" prefWidth="156.0" text="Roll Number" />
            <Label alignment="CENTER" prefHeight="31.0" prefWidth="156.0" text="Branch" />
         </children></VBox>
      <MenuBar layoutY="-1.0" prefHeight="32.0" prefWidth="1000.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
