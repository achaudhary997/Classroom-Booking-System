            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <VBox layoutY="94.0" prefHeight="606.0" prefWidth="156.0">
         <children>
            <Button mnemonicParsing="false" onAction="#showLoginPage" prefHeight="43.0" prefWidth="156.0" text="Return to Login" />
         </children>
      </VBox>
      <Separator layoutX="153.0" layoutY="31.0" orientation="VERTICAL" prefHeight="668.0" prefWidth="0.0" />
      <Button layoutX="457.0" layoutY="585.0" mnemonicParsing="false" onAction="#handleFinalPage" text="Submit" />
